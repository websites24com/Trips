{
  "name": "trips",
  "version": "1.0.0",
  "description": "Trips Offers",
  "main": "app.js",
  "targets": {
    "public": {
      "context": "browser",
      "distDir": "public/dist/js",
      "includeNodeModules": true,
      "sourceMap": false
    }
  },
  "scripts": {
    "start": "node server.js",
    "nodemon": "nodemon server.js",
    "start:prod": "NODE_ENV=production node server.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "debug": "ndb node server.js",
    "parcel:watch": "parcel watch public/js/main.js --target public",
    "parcel:build": "parcel build public/js/main.js --target public",
    "dev": "concurrently -k -n SERVER,PARCEL -c green,cyan \"nodemon server.js\" \"npm:parcel:watch\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/websites24com/Trips.git"
  },
  "author": "www.websites-24.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/websites24com/Trips/issues"
  },
  "homepage": "https://github.com/websites24com/Trips#readme",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@exortek/express-mongo-sanitize": "^1.1.1",
    "@stripe/stripe-js": "^8.4.0",
    "axios": "^1.13.1",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.20.0",
    "mongoose": "^8.18.3",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.9",
    "pug": "^3.0.3",
    "qs": "^6.14.0",
    "sharp": "^0.34.5",
    "slugify": "^1.6.6",
    "stripe": "^19.3.1",
    "validator": "^13.15.15",
    "xss-sanitize": "^1.1.1",
    "parcel": "^2.16.0",
    "buffer": "^6.0.3",
    "concurrently": "^9.2.1",
    "crypto-browserify": "^3.12.1"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "events": "^3.3.0",
    "https-browserify": "^1.0.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.6.2",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.4",
    "vm-browserify": "^1.1.2"
  }
}
